@article{stepniewski_jpcssp_1980,
  author={R Stepniewski and K Pastor and M Grynberg},
  title={Gamma 8 -bands anisotropy and selection rules for far-infrared magneto-optical transitions in $\mathrm{HgTe}$},
  journal={Journal of Physics C: Solid State Physics},
  volume={13},
  number={31},
  pages={5783},
  url={http://stacks.iop.org/0022-3719/13/i=31/a=020},
  year={1980},
  abstract={Far-infrared (FIR) magnetotransmission spectra of n-type $\mathrm{HgTe}$ in Voigt configuration (polarised light; E perpendicular to B and E//B) for different directions of magnetic field in the (110) plane have been measured. The experiment was done in a magnetic field up to 5T at liquid helium temperature. Intraband and interband ( Gamma 8 v to Gamma 8 c ) transitions were observed. The experimental data were analysed in the Luttinger (quasigermanium) approximation. Observed strong anisotropy effects in interband transitions are very well described by means of the 'interaction' between the hole Landau levels induced by the D 1 and D 2 terms in Luttinger notation. From the fitting of the theoretical model to the experimental data the Luttinger band structure parameters were evaluated: gamma 1 =-16.8+or-0.2, gamma 2 =-10.6+or-0.1, gamma 3 =-8.8+or-0.1, kappa =-10.1+or-0.2, q=-0.5+or-0.1. A small disagreement in the absolute value of calculated and observed transition energies as well as the negative sign of the q parameter may be connected with the non-local electron-electron interaction near the Gamma point.}
}

